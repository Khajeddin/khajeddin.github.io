  "license": "MIT",
  "scripts": {
    "build": "env GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "develop": "env GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "gh-pages-action": "gh-pages -d public -b main -x -r https://$GH_TOKEN@github.com/khajeddin/khajeddin.github.io.git --remove '**/*'",
    "deploy": "npm run clean && npm run build && gh-pages -d public -b main --remove '**/*'",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "env GATSBY_TELEMETRY_DISABLED=1 gatsby serve",
    "clean": "env GATSBY_TELEMETRY_DISABLED=1 gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khajeddin/khajeddin.github.io"
  },
  "bugs": {
    "url": "https://github.com/khajeddin/khajeddin.github.io/issues"
  }
