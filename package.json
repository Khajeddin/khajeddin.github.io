  "devDependencies": {
    "@types/node": "^14.18.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^16.9.14",
    "@types/react-helmet": "^6.1.5",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.25",
